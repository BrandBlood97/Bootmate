<div class="container d-flex text-center w-100">
  <div class="d-flex align-items-center w-100">
    <div class="col-6">
      <img
        src="/images/background-UCF.png"
        class="img-"
        alt="image of class logo"
      />
    </div>
    <br />
    <div class="col-6 border" id="search-user">
      <h2>Search for Existing User:</h2>
      <br />
      <form class="search-form">
        <div class="form-floating mb-3">
          <input
            type="text"
            class="form-control"
            id="firstName"
            name="firstName"
            placeholder=""
          />
          <label for="floatingFirstName">First Name</label>
        </div>
        <p>and / or:</p>
        <div class="form-floating">
          <input
            type="text"
            class="form-control"
            id="lastName"
            name="lastName"
            placeholder=""
          />
          <label for="floatingLastName">Last Name</label>
        </div>
        <br />
        <button type="submit" class="btn btn-secondary">Submit</button>
      </form>
      <div class="search-results">
      </div>
    </div>
  </div>
</div>

<script src="./js/search.js"></script>