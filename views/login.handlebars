<div class="container text-center">
  <div class="row">
    <div class="col-4 border" id="new-user">
      <h2>New User Registration:</h2>
      <a
        href="/signup"
        class="btn btn-secondary"
        tabindex="-1"
        role="button"
        aria-disabled="true"
      >Enter</a>
    </div>
    <div class="col-4">
      <img
        src="/images/background-UCF.png"
        class="img-fluid"
        alt="image of class logo"
      />
    </div>
    <div class="col-4 border" id="existing-user">
      <h2>Existing User</h2>
      <h2>Login-in:</h2>
      <form class="login-form">
        
      <div class="form-floating mb-3">
        <label for="email-login">Email address</label>
        <input
          type="email"
          class="form-control"
          id="email-login"
          placeholder="name@example.com"
        />
      </div>
      <div class="form-floating">
        <label for="password-login">Password</label>
        <input
          type="password"
          class="form-control"
          id="password-login"
          placeholder="Password"
        />
      </div>
      <button
        class="btn btn-secondary"
        type="submit"
      >Submit</button>
      </form>
    </div>
  </div>
</div>

<script src="./js/login.js"></script>